generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model book {
  id     Int    @id @default(autoincrement())
  name   String
  author user   @relation(fields: [userId], references: [id])
  userId String
}

model user {
  id       String @id @default(uuid())
  username String
  password String
  book     book[]
}
